<?xml version="1.0" encoding="utf-8"?>
<LinearLayout
    xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    xmlns:tools="http://schemas.android.com/tools"
    android:id="@+id/main"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    tools:context=".page5"
    android:weightSum="10"
    android:orientation="vertical">

    <LinearLayout
        android:layout_width="match_parent"
        android:layout_height="0dp"
        android:orientation="vertical"
        tools:ignore="Suspicious0dp"
        android:layout_weight="1.2"
        >

        <RelativeLayout
            android:layout_width="match_parent"
            android:layout_height="match_parent"
            >

            <ImageView
                android:id="@+id/page5_camera"
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                android:src="@drawable/camera_2"
                android:layout_marginHorizontal="20dp"
                android:layout_marginTop="45dp"
                app:tint="@color/page5_button_color" />
            <TextView
                android:id="@+id/page5_socially"
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                android:text="Socially"
                android:textColor="@color/page5_button_color"
                android:textSize="20dp"
                android:textStyle="bold"
                android:layout_centerHorizontal="true"
                android:layout_marginTop="45dp"
                />

            <ImageView
                android:id="@+id/page5_instalogo"
                android:layout_width="48dp"
                android:layout_height="36dp"
                android:layout_marginHorizontal="70dp"
                android:layout_marginStart="59dp"
                android:layout_marginTop="45dp"
                android:layout_toEndOf="@+id/page5_socially"
                android:src="@drawable/insta_logo"
                app:tint="@color/page5_button_color" />
            <ImageView
                android:id="@+id/page5_message"
                android:layout_width="52dp"
                android:layout_height="33dp"
                android:layout_marginTop="45dp"
                android:layout_alignParentRight="true"
                android:src="@drawable/message_arrow"

                app:tint="@color/page5_button_color" />
            <View
                android:id="@+id/bar"
                android:layout_width="match_parent"
                android:layout_height="1dp"
            android:layout_marginBottom="1dp"
            android:layout_alignParentBottom="true"

            android:background="#000000" /> <!-- pure black -->


        </RelativeLayout>

    </LinearLayout>

    <LinearLayout
        android:layout_width="match_parent"
        android:layout_height="0dp"
        android:layout_weight="1.4"
        android:orientation="vertical"
        tools:ignore="Suspicious0dp">

        <RelativeLayout
            android:layout_width="match_parent"
            android:layout_height="match_parent">
            <!--My story  -->

            <de.hdodenhof.circleimageview.CircleImageView
                android:id="@+id/your_story"
                android:layout_width="75dp"
                android:layout_height="69dp"
                android:layout_marginTop="5dp"
                android:src="@drawable/page3_image"
                app:civ_border_color="#DD2A7B"
                app:civ_border_width="2dp" />

            <TextView
                android:id="@+id/my_story_text"
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                android:text="Your Story"
                android:textStyle="bold"
                android:layout_marginHorizontal="7dp"
                android:layout_below="@+id/your_story"
                />

            <!--Person 1  -->

            <!--Person 2  -->

            <!--Person 3  -->

            <de.hdodenhof.circleimageview.CircleImageView
                android:id="@+id/person1_story"
                android:layout_width="75dp"
                android:layout_height="69dp"
                android:layout_marginHorizontal="5dp"
                android:layout_marginStart="-5dp"
                android:layout_marginTop="5dp"
                android:layout_toEndOf="@+id/my_story_text"
                android:src="@drawable/person1_ppf"
                app:civ_border_color="#DD2A7B"
                app:civ_border_width="2dp" />

            <TextView
                android:id="@+id/person1_story_text"
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                android:layout_below="@+id/person1_story"
                android:layout_marginHorizontal="7dp"
                android:layout_marginStart="4dp"
                android:layout_marginTop="1dp"
                android:layout_toEndOf="@+id/my_story_text"
                android:text="Karenne"
                android:textStyle="bold" />

            <de.hdodenhof.circleimageview.CircleImageView
                android:id="@+id/person2_story"
                android:layout_width="75dp"
                android:layout_height="69dp"
                android:layout_marginHorizontal="5dp"
                android:layout_marginStart="10dp"
                android:layout_marginTop="5dp"
                android:layout_toEndOf="@+id/person1_story_text"
                android:src="@drawable/person2_ppf"
                app:civ_border_color="#DD2A7B"
                app:civ_border_width="2dp" />

            <!--Person 3  -->

            <TextView
                android:id="@+id/person2_story_text"
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                android:layout_below="@+id/person2_story"
                android:layout_marginHorizontal="7dp"
                android:layout_marginStart="15dp"
                android:layout_marginTop="0dp"
                android:layout_toEndOf="@+id/person1_story_text"
                android:text="Namrood"
                android:textStyle="bold" />

            <de.hdodenhof.circleimageview.CircleImageView
                android:id="@+id/person3_story"
                android:layout_width="75dp"
                android:layout_height="69dp"
                android:layout_marginHorizontal="5dp"
                android:layout_marginStart="-3dp"
                android:layout_marginTop="5dp"
                android:layout_toEndOf="@+id/person2_story"
                android:src="@drawable/person3_ppf"
                app:civ_border_color="#DD2A7B"
                app:civ_border_width="2dp" />

            <TextView
                android:id="@+id/person3_story_text"
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                android:layout_below="@+id/person3_story"
                android:layout_marginHorizontal="7dp"
                android:layout_marginStart="10dp"
                android:layout_marginTop="-1dp"
                android:layout_toEndOf="@+id/person2_story_text"
                android:text="Abdullah"
                android:textStyle="bold" />

            <de.hdodenhof.circleimageview.CircleImageView
                android:id="@+id/person4_story"
                android:layout_width="75dp"
                android:layout_height="69dp"
                android:layout_marginHorizontal="5dp"
                android:layout_marginStart="-2dp"
                android:layout_marginTop="5dp"
                android:layout_toEndOf="@+id/person3_story"
                android:src="@drawable/person4_ppf"
                app:civ_border_color="#DD2A7B"
                app:civ_border_width="2dp" />

            <TextView
                android:id="@+id/person4_story_text"
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                android:layout_below="@+id/person4_story"
                android:layout_marginHorizontal="10dp"
                android:layout_marginStart="9dp"
                android:layout_marginTop="1dp"
                android:layout_toEndOf="@+id/person3_story_text"
                android:text="Ali_Ramish"
                android:textStyle="bold" />


        </RelativeLayout>

    </LinearLayout>

    <LinearLayout
        android:layout_width="match_parent"
        android:layout_height="0dp"
        android:orientation="vertical"
        tools:ignore="Suspicious0dp"
        android:layout_weight="0.8"
        >

        <RelativeLayout
            android:layout_width="match_parent"
            android:layout_height="match_parent">

            <de.hdodenhof.circleimageview.CircleImageView
                android:id="@+id/feed_person"
                android:layout_width="78dp"
                android:layout_height="69dp"
                android:layout_marginHorizontal="5dp"
                android:layout_marginStart="14dp"
                android:layout_marginTop="2dp"
                android:src="@drawable/feed_person"
                app:civ_border_color="#DD2A7B"
                app:civ_border_width="2dp" />
            <TextView
                android:id="@+id/feed_person_name"
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                android:text="Joshua_I"
                android:layout_toEndOf="@+id/feed_person"
                android:textStyle="bold"
                android:layout_marginTop="15dp"
                android:layout_marginHorizontal="10dp"
                />
            <TextView
                android:id="@+id/feed_location"
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                android:text="Tokyo, japan"
                android:layout_below="@+id/feed_person_name"
                android:layout_marginHorizontal="100dp"
                tools:ignore="UnknownId" />

            <ImageView
                android:id="@+id/setting_dots"
                android:layout_width="30dp"
                android:layout_height="40dp"
                android:layout_centerHorizontal="true"
                android:layout_alignParentRight="true"
                android:layout_marginRight="33dp"
                android:src="@drawable/threedots"
                app:tint="@color/black"

                />


        </RelativeLayout>




    </LinearLayout>
    <LinearLayout
        android:layout_width="match_parent"
        android:layout_height="0dp"
        android:orientation="vertical"
        tools:ignore="Suspicious0dp"
        android:layout_weight="4"
        >

        <RelativeLayout
            android:layout_width="match_parent"
            android:layout_height="match_parent">

            <ImageView
                android:id="@+id/post"
                android:layout_width="wrap_content"
                android:layout_height="match_parent"
                android:src="@drawable/post"

                />

            <Button
                android:layout_width="31dp"
                android:layout_height="30dp"
                android:layout_alignParentRight="true"
                android:layout_marginTop="50dp"
                android:layout_marginRight="20dp"
                android:backgroundTint="@color/page5_button_color"
                android:textColor="@color/black" />

            <TextView
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                android:layout_alignParentRight="true"
                android:layout_marginTop="53dp"
                android:layout_marginRight="23dp"
                android:text="1/3"

                />


        </RelativeLayout>

    </LinearLayout>


    <LinearLayout
        android:layout_width="match_parent"
        android:layout_height="0dp"
        android:orientation="vertical"
        tools:ignore="Suspicious0dp"
        android:layout_weight="0.6"
        >
        <RelativeLayout
            android:layout_width="match_parent"
            android:layout_height="match_parent"
            >

            <ImageView
                android:id="@+id/page5_like"
                android:layout_width="28dp"
                android:layout_height="37dp"
                android:src="@drawable/heart_blank"
                android:layout_marginHorizontal="5dp"
                />

            <ImageView
                android:id="@+id/page5_comment"
                android:layout_width="28dp"
                android:layout_height="37dp"
                android:layout_marginHorizontal="10dp"
                android:layout_marginLeft="13dp"
                android:layout_toRightOf="@+id/page5_like"
                android:src="@drawable/comment" />

            <ImageView
                android:id="@+id/page5_share"
                android:layout_width="28dp"
                android:layout_height="37dp"
                android:src="@drawable/message_arrow"
                android:layout_toRightOf="@+id/page5_comment"

                />
            <ImageView
                android:id="@+id/page5_bookmark"
                android:layout_width="28dp"
                android:layout_height="37dp"
                android:src="@drawable/bookmark"
                android:layout_alignParentRight="true"
                android:layout_marginRight="10dp"

                />

            <ImageView
                android:id="@+id/setting_dots2"
                android:layout_width="30dp"
                android:layout_height="40dp"
                android:layout_centerHorizontal="true"

                android:src="@drawable/threedots"
                app:tint="@color/page5_button_color"

                />

        </RelativeLayout>


    </LinearLayout>


    <LinearLayout
        android:layout_width="match_parent"
        android:layout_height="0dp"
        android:layout_weight="0.9"
        android:orientation="vertical"
        tools:ignore="Suspicious0dp"

        >

        <RelativeLayout
            android:layout_width="match_parent"
            android:layout_height="match_parent">

            <de.hdodenhof.circleimageview.CircleImageView
                android:id="@+id/liked"
                android:layout_width="58dp"
                android:layout_height="28dp"
                android:layout_marginTop="2dp"
                android:src="@drawable/person2_ppf"
                app:civ_border_color="#DD2A7B"
                app:civ_border_width="2dp" />

            <TextView
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                android:text="Liked by Namrood and 44,686 others"
                android:layout_toRightOf="@+id/liked"
                android:layout_marginTop="10dp"
                />

            <TextView
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                android:text="Namrood The Nature in Japan was beautiful and i \nwant to share some photos  "
                android:layout_alignParentBottom="true"
                android:layout_marginHorizontal="25dp"
                android:layout_marginTop="2dp"
                />

            <View
                android:id="@+id/bar2"
                android:layout_width="match_parent"
                android:layout_height="1dp"
                android:layout_alignParentBottom="true"
                android:background="#000000" />


        </RelativeLayout>

    </LinearLayout>

    <LinearLayout
        android:layout_width="match_parent"
        android:layout_height="0dp"
        android:layout_weight="1.1"
        android:orientation="vertical"
        tools:ignore="Suspicious0dp"

        >
        <RelativeLayout
            android:layout_width="match_parent"
            android:layout_height="match_parent">

            <ImageView
                android:id="@+id/home_page"
                android:layout_width="90dp"
                android:layout_height="69dp"
                android:src="@drawable/home_filled"
                app:tint="@color/page5_button_color" />

            <ImageView
                android:id="@+id/search_btn"
                android:layout_width="90dp"
                android:layout_height="69dp"
                android:layout_marginLeft="-17dp"
                android:layout_toRightOf="@+id/home_page"
                android:src="@drawable/search"
                app:tint="@color/page5_button_color" />

            <ImageView
                android:id="@+id/post_btn"
                android:layout_width="90dp"
                android:layout_height="69dp"
                android:layout_marginLeft="-13dp"
                android:layout_toRightOf="@+id/search_btn"
                android:src="@drawable/post_icon"
                app:tint="@color/page5_button_color" />

            <ImageView
                android:id="@+id/heart_btn"
                android:layout_width="67dp"
                android:layout_height="33dp"
                android:layout_marginLeft="-14dp"
                android:layout_marginTop="12dp"
                android:layout_toRightOf="@+id/post_btn"
                android:src="@drawable/heart_blank"
                app:tint="@color/page5_button_color" />

            <de.hdodenhof.circleimageview.CircleImageView
                android:id="@+id/profile_btn"
                android:layout_width="35dp"
                android:layout_height="53dp"
                android:layout_alignParentRight="true"

                android:layout_marginTop="2dp"

                android:layout_marginRight="21dp"
                android:src="@drawable/person2_ppf"
                app:civ_border_color="#DD2A7B"
                app:civ_border_width="2dp" />

        </RelativeLayout>


    </LinearLayout>

</LinearLayout>